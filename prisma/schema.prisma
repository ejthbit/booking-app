generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model bookings {
  id        Int     @id @default(autoincrement())
  contact   Json?   @db.Json
  name      String? @db.VarChar(255)
  birthdate String? @db.VarChar(255)
  workplace Int
  end       String? @db.VarChar(255)
  start     String? @db.VarChar(255)
}

model doctorServices {
  id         Int      @id @default(autoincrement())
  created_at DateTime @default(now()) @db.Timestamp(6)
  month      String   @db.VarChar(255)
  days       Json?    @db.Json
  workplace  Int
}

model users {
  id                Int      @id @default(autoincrement())
  created_at        DateTime @default(now()) @db.Timestamp(6)
  email             String   @db.VarChar(255)
  password          String
  name              String?  @db.VarChar(255)
  default_workplace Int?
}

model workplaces {
  id           Int      @id @default(autoincrement())
  created_at   DateTime @default(now()) @db.Timestamp(6)
  name         String   @db.VarChar(255)
  full_name    String?  @db.VarChar(255)
  address      String?
  workplace_id Int
  contact      Json?    @db.Json
}

model doctors {
  id                      Int      @id @default(autoincrement())
  created_at              DateTime @default(now()) @db.Timestamp(6)
  doctor_id               Int
  name                    String   @db.VarChar(255)
  workplace_id            String[]
  preferred_service_start String?  @db.VarChar(5)
}

model sonography {
  id         Int      @id @default(autoincrement())
  created_at DateTime @default(now()) @db.Timestamp(6)
  date       String   @db.VarChar(255)
  from       String   @db.VarChar(255)
  to         String   @db.VarChar(255)
}
